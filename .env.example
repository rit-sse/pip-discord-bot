DISCORD_TOKEN=user-hash.login-process-hash.token-hash
TEST_CLIENT_ID=000000000000000000
TEST_SERVER_ID=000000000000000000

WEBSERVER_PORT=3000
GOOGLE_CLIENT_ID=xx-xx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-secret
REDIRECT_URI=http://localhost:${WEBSERVER_PORT}/api/auth/
SIGNING_SECRET=

# ----- DO NOT EDIT BELOW THIS LINE -----
# -----  FOR CROSS-ENV CONSISTENCY  -----

POSTGRES_DB=pipbotdb
POSTGRES_USER=pipbotuser
# This line may be edited, but after each edit, use `docker-compose up --build` (this may destroy your old DB)
# Please use a password policy, I reccomend 32 char min, alphanumeric
POSTGRES_PASSWORD=a_very_secure_password_please_change

DATABASE_URL=postgresql://POSTGRES_USER:{POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}
